<%- include('../layout/header.ejs') %>

    <form method="post" action="../actualizar/", enctype="multipart/form-data">

        <div class="form-group">
            <label for="my-input">ID:</label>
            <input id="id" class="form-control" type="text" name="id" value="<%= beneficiario.idBeneficiario%>" readonly>
        </div>
        <div class="form-group">
            <label for="my-input">Nombre:</label>
            <input id="nombre" class="form-control" type="text" name="nombre" value="<%= beneficiario.nombre %>">
        </div>
        <div class="form-group">
            <label for="my-input">RFC:</label>
            <input id="rfc" class="form-control" type="text" name="rfc" value="<%= beneficiario.rfc %>">
        </div>
        <button class="btn btn-primary" type="submit">Guardar</button>
        
    </form>
<%- include('../layout/footer.ejs') %>


